    "lighthouse:assert": "lhci assert",
    "lighthouse:upload": "lhci upload",
    "production-check": "node scripts/production-check.js",
    "pre-deploy": "pnpm production-check && pnpm build",
    "ci": "pnpm install --frozen-lockfile && pnpm type-check && pnpm lint && pnpm test --passWithNoTests && pnpm build",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:e2e:ci": "echo 'E2E tests placeholder - to be implemented'",
